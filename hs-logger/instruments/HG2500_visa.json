{
    "instrument_id": "HG2500",
    "instrument_name": "Thunder Scientific 2500 Humidity Generator",
    "driver": "generic_driver_visa_serial",
    "interface": "visa",
    "port": "COM7",
    "baudrate": 2400,
    "write_termination": "\r",
    "read_termination": "\r\n",
    "notes": "",
    "operations": {
        "read_default":{
            "type": "read_multiple",
            "name": "Actual/generated values",
            "id": "read_default",
            "details":"",
            "command":"?",
            "operations":["read_rh_pc", "read_rh_pctc", "read_ts", "read_tc", "read_ps", "read_pc", "read_q", "read_status"],
            "data_type": "list",
            "split":",",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_rh_pc":{
            "type": "read_store",
            "name": "%rh @Pc 2500",
            "id": "read_rh_pc",
            "details":"",
            "store_id": "read_default",
            "store_index": 0,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_rh_pctc":{
            "type": "read_store",
            "name": "%rh @PcTc 2500",
            "id": "read_rh_pctc",
            "details":"",
            "store_id": "read_default",
            "store_index": 1,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_ps":{
            "type": "read_store",
            "name": "Ps 2500",
            "id": "read_ps",
            "details":"",
            "store_id": "read_default",
            "store_index": 2,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["P", 0, 0.0689476, 0, 0]
        },
        "read_pc":{
            "type": "read_store",
            "name": "Pc 2500",
            "id": "read_pc",
            "details":"",
            "store_id": "read_default",
            "store_index": 3,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["P", 0, 0.0689476, 0, 0]
        },
        "read_ts":{
            "type": "read_store",
            "name": "Ts 2500",
            "id": "read_ts",
            "details":"",
            "store_id": "read_default",
            "store_index": 4,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_tc":{
            "type": "read_store",
            "name": "Tc 2500",
            "id": "read_tc",
            "details":"",
            "store_id": "read_default",
            "store_index": 5,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_q":{
            "type": "read_store",
            "name": "Q 2500",
            "id": "read_q",
            "details":"",
            "store_id": "read_default",
            "store_index": 6,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_status":{
            "type": "read_store",
            "name": "Status 2500",
            "id": "read_status",
            "details":"",
            "store_id": "read_default",
            "store_index": 7,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_setpoints":{
            "type": "read_multiple",
            "name": "Setpoints",
            "id": "read_setpoints",
            "details":"Returns current system gsetpoint values",
            "command":"?SP",
            "operations":["read_rh_pc_sp", "read_rh_pctc_sp", "read_ts_sp", "read_ps_sp", "read_q_sp", "read_mode"],
            "data_type": "list",
            "split":",",
            "unit": ""
        },
        "read_rh_pc_sp":{
            "type": "read_store",
            "name": "%rh @Pc 2500 setpoint",
            "id": "read_rh_pc_sp",
            "details":"",
            "store_id": "read_setpoints",
            "store_index": 0,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_rh_pctc_sp":{
            "type": "read_store",
            "name": "%rh @PcTc 2500 setpoint",
            "id": "read_rh_pctc_sp",
            "details":"",
            "store_id": "read_setpoints",
            "store_index": 1,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_ps_sp":{
            "type": "read_store",
            "name": "Ps 2500 setpoint",
            "id": "read_ps_sp",
            "details":"",
            "store_id": "read_setpoints",
            "store_index": 2,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_ts_sp":{
            "type": "read_store",
            "name": "Ts 2500 setpoint",
            "id": "read_ts_sp",
            "details":"",
            "store_id": "read_setpoints",
            "store_index": 3,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_q_sp":{
            "type": "read_store",
            "name": "Q 2500 setpoint",
            "id": "read_q_sp",
            "details":"",
            "store_id": "read_setpoints",
            "store_index":4,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "read_mode":{
            "type": "read_store",
            "name": "Mode 2500",
            "id": "read_mode",
            "details":"",
            "store_id": "read_setpoints",
            "store_index": 5,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["V", 0, 1, 0, 0]
        },
        "action_run":{
            "type": "action",
            "name": "Run 2500",
            "id":   "action_run",
            "details": "Start the System",
            "command": "RUN"
        },
        "action_stop":{
            "type": "action",
            "name": "Stop 2500",
            "id":   "action_stop",
            "details": "Stops the System",
            "command": "STO"
        },
        "set_state":{
            "type": "write_action",
            "name": "Operation state",
            "id":   "set_state",
            "details": "0 to shutdown, 1 to generate",
            "operations": {"0": "action_stop","1": "action_run"},
            "command": "{}",
            "check_set": "read_mode",
            "check_actual": "read_status"
        },
        "set_rh_pc":{
            "type": "write",
            "name": "RH @PC 2500 set",
            "id":   "set_rh_pc",
            "command": "R1={}",
            "check_set": "read_rh_pc_sp",
            "check_actual": "read_rh_pc"
        },
        "set_rh_pctc":{
            "type": "write",
            "name": "RH @PcTc 2500 set",
            "id":   "set_rh_pctc",
            "command": "R2={}",
            "check_set": "read_rh_pctc_sp",
            "check_actual": "read_rh_pctc"
        },
        "set_q":{
            "type": "write",
            "name": "Q 2500 set",
            "id":   "set_q",
            "command": "FS={}",
            "check_set": "read_q_sp",
            "check_actual": "read_q"
        },
        "set_ps":{
            "type": "write",
            "name": "Ps 2500 set",
            "id":   "set_ps",
            "command": "PS={}",
            "check_set": "read_ps_sp",
            "check_actual": "read_ps"
        },
        "set_ts":{
            "type": "write",
            "name": "Ts 2500 set",
            "id":   "set_ts",
            "command": "TS={}",
            "check_set": "read_ts_sp",
            "check_actual": "read_ts"
        }
    }
}