{
  "instrument_id": "Julabo_HL",
    "instrument_name": "Julabo HL",
    "driver": "julabo_py_serial",
    "interface": "pyserial",
    "port": "COM4",
    "baudrate": 4800,
    "parity": "PARITY_EVEN",
    "stopbits": "STOPBITS_ONE",
    "timeout": 3,
    "write_termination":"\r",
    "read_termination":"\r\n",
    "echo": false,
    "notes": " ",
    "operations": {
        "read_Tm": {
            "type": "read_single",
            "name": "Tb Julabo HL",
            "id": "read_Tm",
            "command": "IN_PV_00",
            "transform_eq":["V",0,1,0,0],
            "data_type": "float",
            "unit": "deg C",
            "details": "Tm is internal temperature"
        },
        "read_Tsp1": {
            "type": "read_single",
            "name": "Tsp1 Julabo HL",
            "id": "read_Tsp1",
            "command": "IN_SP_00",
            "transform_eq":["V",0,1,0,0],
            "data_type": "float",
            "unit": "deg C",
            "details": "Tsp1 is set point temperature"
        },
        "read_Unit_on": {
            "type": "read_single",
            "name": "Julabo HL On",
            "id": "read_Unit_on",
            "command": "IN_MODE_05",
            "transform_eq":["V",0,1,0,0],
            "data_type": "bit",
            "unit": "NA",
            "details": "Read unit on state"
        },
        "write_Tsp1": {
            "type": "write",
            "name": "Tsp1 Julabo HL",
            "id": "write_Tsp1",
            "command": "OUT_SP_00 {}",
            "details": "Tsp1 is set point temperature"
        },
        "write_unit_on": {
            "type": "write",
            "name": "Julabo HL on",
            "id": "write_unit_on",
            "command": "OUT_MODE_05 {}",
            "details": "Set Unit On Status, 1 on and 0 off"
        },
        "write_pump_stage": {
            "type": "write",
            "name": "Julabo Pump Stage",
            "id": "write_pump_stage",
            "command": "OUT_SP_07 {}",
            "details": "Set pump stage to an integer from 1 to 4."
        }
    }
}