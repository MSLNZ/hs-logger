        },
        "action_current_sqrt2": {
            "type": "action",
            "name": "Current Sqrt(2)",
            "id": "action_current_sqrt2",
            "details": "Sets the read current to sqrt(2) A",
            "command": "SENS:CURR ROOT2"
        },
