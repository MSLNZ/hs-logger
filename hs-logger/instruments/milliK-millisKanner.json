        "action_temp_setup": {
            "type": "action",
            "name": "Temporary Setup",
            "id": "action_temp_setup",
            "details": "Sets the device up to read resistance (TEMPORARY)",
            "command": "SENS:FUNC RES\nSENS:CURR NORM\nSENS:RES:RANG 200\nSENS:WIR 4"
        },
        "action_read_resistance": {
            "type": "action",
