{
    "instrument_id": "milliK",
    "instrument_name": "Isotech milliK with attached millisKanner",
    "driver": "generic_driver_visa_serial",
    "interface": "visa",            
    "port": "COM4",
    "baudrate":9600,
    "write_termination": "\r",
    "read_termination": "\r",
    "notes": "",
    "operations": {
        "read_default": {
            "type": "read_multiple",
            "name": "All channels",
            "id": "read_default",
            "details": "",
            "command": "SENS:CHAN 10\nREAD?\nSENS:CHAN 11\nREAD?\nSENS:CHAN 13\nREAD?\nSENS:CHAN 13\nREAD?\nSENS:CHAN 14\nREAD?\nSENS:CHAN 15\nREAD?\nSENS:CHAN 16\nREAD?\nSENS:CHAN 17\nREAD?\n",
            "operations": ["read_10", "read_11", "read_12", "read_13", "read_14", "read_15", "read_16", "read_17"],
            "data_type": "list",
            "split": ",",
            "unit": ""
        },
        "read_test": {
            "type": "read_multiple",
            "name": "Read a channel",
            "id": "read_test",
            "details": "",
            "command": "MEAS:RES10? 100,NORM,4",
            "operations": ["read_0"],
            "data_type": "list",
            "split": ",",
            "unit": ""
        },
        "read_1": {
            "type": "read_store",
            "name": "Channel 1",
            "id": "read_1",
            "details": "",
            "store_id": "read_test",
            "store_index": 0,
            "data_type": "float",
            "unit": "",
            "transform_eq": ["T", 0.004, -0.0000006, -0.000000000005, 100]
        },
        "read_10": {
            "type": "read_store",
