            "command": "!Gasi{}",
            "data_type": "int",
            "unit": "index",
            "details": "write gas index ->see manual 1 - Air \n\t\t2 - Argon\n\t\t3 - CO2\n\t\t4 - CO\n\t\t5 - He\n\t\t6 - H\n\t\t7 - CH4\n\t\t8 - N\n\t\t9 - NO\n\t\t10 - O"
        },
        "write_stream_mode": {
            "type": "write",
            "name": "Stream_mode C2",
            "id": "Stream_mode",
            "command": "!Strm{}",
            "data_type": "string",
            "unit": "string",
            "details": "streamString = \"On\", \"Off\", \"Echo\""
